  "scripts": {
    "clean": "node scripts/clean.mjs",
    "build": "node scripts/build.mjs",
    "watch": "node scripts/watch.mjs",
    "serve": "node scripts/serve.mjs config/server.config.json",
    "serve-saml": "node scripts/serve.mjs config/server.config.saml-local.json",
    "build-comment": "NODE_ENV=production rollup -c rollup/comment.mjs",
    "build-desktop": "rollup -c rollup/desktop.mjs",
    "build-electron-installer": "NODE_ENV=production npx electron-builder -c ./electron-builder-config.json",
    "build-servers": "NODE_ENV=production rollup -c rollup/server.mjs",
    "build-extension": "NODE_ENV=production rollup -c rollup/extension.mjs",
    "start-desktop": "electron . --open-dev-tools --verbose --no-sandbox",
    "lint": "eslint --ext ts,tsx --max-warnings 0 src",
    "saml-idp": "saml-idp --acs http://localhost:7000/auth/saml --aud mock-audience --key config/DO_NOT_USE-idp-key.pem --cert config/DO_NOT_USE-idp-cert.pem"
  },
  "dependencies": {
