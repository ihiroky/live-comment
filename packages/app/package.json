{
  "name": "live-comment",
  "version": "0.4.0",
  "private": true,
  "main": "dist/desktop/index.js",
  "homepage": "./",
  "repository": {
    "type": "git",
    "url": "https://github.com/ihiroky/live-comment.git"
  },
  "author": "Hiroki Itoh <ihiroky@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "node scripts/clean.mjs",
    "watch": "node scripts/clean.mjs && node scripts/watch.mjs",
    "watch-comment": "concurrently \"rollup -c rollup.comment.js -w\" \"node scripts/serve.mjs\"",
    "build-comment": "tsc -b && NODE_ENV=production rollup -c rollup.comment.js",
    "watch-desktop": "concurrently \"tsc -p tsconfig-desktop.json -w\" \"rollup -c rollup.desktop.js -w\"",
    "start-desktop": "electron . --open-dev-tools --debug",
    "build-desktop": "tsc -p tsconfig-desktop.json && rollup -c rollup.desktop.js",
    "watch-streaming": "concurrently \"tsc -w -p tsconfig-server.json\" \"ncc build -w -o dist/bundle/server/streaming dist/server/streaming.js\"",
    "start-streaming": "node dist/bundle/server/streaming/index.js -l DEBUG -p 8080",
    "build-streaming": "tsc -p tsconfig-server.json && ncc build -m -o dist/bundle/server/streaming dist/server/streaming.js",
    "watch-api": "concurrently \"tsc -w -p tsconfig-server.json\" \"ncc build -w -o dist/bundle/server/api dist/server/api.js\"",
    "start-api": "node dist/bundle/server/api/index.js -l DEBUG -p 9080",
    "build-api": "tsc -p tsconfig-server.json && ncc build -m -o dist/bundle/server/api dist/server/api.js",
    "lint": "eslint --ext ts,tsx --max-warnings 0 src"
  },
  "dependencies": {
    "@mui/material": "^5.1.1",
    "@mui/styles": "^5.1.1",
    "@types/cors": "^2.8.12",
    "@types/dompurify": "^2.2.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^16.0.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.0.0",
    "@types/ws": "^7.2.6",
    "@types/yargs": "^16.0.1",
    "chart.js": "^3.0.0",
    "cors": "^2.8.5",
    "dompurify": "^2.2.7",
    "express": "^4.17.1",
    "js-logger": "^1.6.1",
    "js-sha512": "^0.8.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "react": "^17.0.2",
    "react-chartjs-2": "^3.0.3",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "unzip": "1.0.0",
    "ws": "^8.2.3",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.1",
    "@testing-library/user-event": "^13.2.1",
    "@vercel/ncc": "^0.33.4",
    "concurrently": "^6.2.0",
    "electron": "^12.1.2",
    "electron-builder": "^22.10.5",
    "nodemon": "^2.0.16",
    "rollup": "^2.56.3",
    "rollup-plugin-terser": "^7.0.2",
    "supertest": "^6.1.6",
    "tslib": "^2.0.0",
    "typescript": "^4.4.2"
  },
  "optionalDependencies": {
    "@emotion/react": "^11.6.0",
    "@emotion/styled": "^11.6.0",
    "bufferutil": "^4.0.3",
    "utf-8-validate": "^5.0.4"
  }
}
