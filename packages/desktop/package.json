{
  "name": "live-comment-desktop",
  "version": "0.1.0",
  "description": "live-comment desktop",
  "main": "dist/js/index.js",
  "homepage": "https://github.com/ihiroky/live-comment",
  "repository": "https://github.com/ihiroky/live-comment/desktop",
  "author": "Hiroki Itoh <ihiroky@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "node -e \"require('fs').rmdirSync('dist', {recursive: true})\"",
    "start": "tsc -b && electron . --enable-logging",
    "build:screen": "yarn --cwd ../screen build",
    "build": "tsc -b && rollup -c && electron-builder -c electron-builder-config.json --dir",
    "package:m": "tsc -b && rollup -c && electron-builder -c electron-builder-config.json -m",
    "package:wl": "tsc -b && rollup -c && electron-builder -c electron-builder-config.json -wl"
  },
  "devDependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-replace": "^2.4.2",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "asar": "^3.0.3",
    "electron": "^12.0.5",
    "electron-builder": "^22.7.0",
    "eslint": "^7.23.0",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.44.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.2.3"
  }
}
