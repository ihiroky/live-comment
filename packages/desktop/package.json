    "@rollup/plugin-replace": "^2.4.2",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "asar": "^3.0.3",
    "concurrently": "^6.2.0",
    "electron": "^12.1.2",
    "electron-builder": "^22.10.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.44.0",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-terser": "^7.0.2",
